@{
    ViewData["Title"] = "Venues";
}

@model ClassLibrary.Value;



<h2>Manage your venues here</h2>


@foreach (ClassLibrary.Venue x in Model.Venues)
{
    <button href="/Home/DeleteVenue" name="@x">@x.VenueName</button><br />
}
<h1>@ViewBag.Message</h1>

@using (Html.BeginForm("DeleteVenue", "Venues", FormMethod.Post))
{
    @Html.DropDownList(
            "Foo",
            new SelectList(
                Model.Venues.Select(x => new { Value = x.VenueId, Text = x.VenueName }),
                "Value",
                "Text"
            )
        )

    <input type="submit" value="Delete"/>
}